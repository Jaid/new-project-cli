    "handlebars": "^4.5.3",
    "has-content": "^1.1.0",
    "header-case": "^1.0.1",
    "is-git-repo-dirty": "^1.0.0",
    "jaid-logger": "^1.5.3",
    "lodash": "^4.17.15",
    "npm-check-updates": "^3.2.2",
    "npm-name-exists": "^1.0.4",
    "open": "^7.0.0",
    "pascal-case": "^2.0.1",
    "pify": "^4.0.1",
    "replace-in-file": "^4.2.0",
    "simple-git": "^1.126.0",
